      "pages": [
        "channels/Platforms",
        {
          "group": "Marketplaces",
          "pages": [
            "channels/India",
            "channels/Middle-East",
            "channels/Asia",
            "channels/North-America",
            "channels/Europe",
            "channels/Oceania",
            "channels/Latin America",
