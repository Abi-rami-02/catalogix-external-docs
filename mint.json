    {
      "group": "Developer Guide",
      "pages": [
        "api-reference/developer/quickstart",
        "api-reference/developer/authentication",
        "api-reference/developer/errors",
        "api-reference/developer/Rate-Limits",
        "api-reference/developer/product-model"
