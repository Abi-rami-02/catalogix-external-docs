{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Catalogix Documentation",
  "logo": {
    "light": "https://cdn.prod.website-files.com/628a7fa3d53df316ad56015b/660f9d2acd58a995b434b157_Catalogix%20Logo.svg",
    "dark": "https://cdn.prod.website-files.com/628a7fa3d53df316ad56015b/660f9d2acd58a995b434b157_Catalogix%20Logo.svg"
  },
  "analytics": {
    "plausible": {
      "domain": "docs.catalogix.ai"
    }
  },
  "api": {
    "baseUrl": "https://api.catalogix.ai/v1/",
    "playground": { "mode": "show" }
  },
  "layout": "sidenav",
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "sidebar": { "items": "container" },
  "topbar": { "style": "default" },
  "search": { "location": "top", "prompt": "Search..." },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:help@catalogix.ai"
    }
  ],
  "topbarCtaButton": {
    "type": "link",
    "name": "Get Started",
    "url": "https://dashboard.catalogix.ai",
    "style": "pill",
    "arrow": true
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    },
    {
      "name": "Changelog",
      "url": "changelog"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": [
        "Introduction",
        "Quickstart",
        {
          "group": "How To",
          "pages": [
            "how-to/Delete a Product",
